#include<iostream>
#include<cstdio>

int main()
{
    //freopen("output.txt","w",stdout);
    int t = 0;
    scanf("%d",&t);
    int a = 0, f = 0;
    while(t--){
        scanf("%d%d",&a,&f);
        while(f--){
            for(int i = 1;i<=a;i++)
             {
                 for(int j = 0;j<i;j++)
                        printf("%d",i);
                    printf("\n");
             }
             for(int i = a-1;i>0;i--)
             {
                 for(int j = 0;j<i;j++)
                        printf("%d",i);
                    printf("\n");
             }
             if(f)
                printf("\n");
        }
        if(t)
            printf("\n");
    }
    return 0;
}
